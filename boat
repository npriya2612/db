1. SELECT sname FROM Sailors S, Reserves R WHERE S.sid=R.sid AND bid=103

2. SELECT S.sname FROM Sailors S, Reserves R, Boats B WHERE S.sid = R.sid AND R.bid = B.bid AND B.color = 'red'.

3. SELECT B.color FROM Sailors S, Reserves R, Boats B WHERE S.sid = R.sid AND R.bid = B.bid AND S.sname = 'Lubber'.

4. SELECT S.sname FROM Sailors S, Reserves R WHERE S.sid = R.sid.

5. SELECT S.sid, S.sname, S.rating, S.age FROM Sailors AS S WHERE S.rating > 7

6. SELECT S.age FROM Sailors S WHERE S.sname LIKE 'B%B'

7. SELECT S.sname FROM Sailors S, Reserves R, Boats B WHERE S.sid=R.sid AND R.bid = B.bid AND (B.color = 'red' OR B.color = 'green')

8. SELECT S.sname FROM Sailors S, Reserves R1, Boats B1, Reserves R2, Boats B2 WHERE S.sid = R1.sid AND R1.bid=B1.bid AND S.sid=R2.sid AND R2.bid=B2.bid AND B1.color='red' AND B2.color = 'green'
